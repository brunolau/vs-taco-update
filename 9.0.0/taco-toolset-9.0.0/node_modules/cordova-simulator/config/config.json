{
  "requirejs": {
    "baseUrl": "/",
    "paths": {
      "app": "js",
      "api": "api",
      "plugins": "plugins"
    },
    "shim": {
      "angular": {
        "exports": "angular",
        "deps": [
          "jquery"
        ]
      },
      "bootstrap": {
        "deps": [
          "jquery"
        ]
      },
      "Blob": {
        "exports": "Blob"
      }
    }
  },
  "libs": {
    "socketio": [
      "https://cdnjs.cloudflare.com/ajax/libs/socket.io/0.9.16/socket.io.min",
      "socket.io/socket.io"
    ],
    "domReady": [
      "https://cdnjs.cloudflare.com/ajax/libs/require-domReady/2.0.1/domReady.min",
      "lib/requirejs-domready/domReady"
    ],
    "jquery": [
      "https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min",
      "lib/jquery/dist/jquery"
    ],
    "bootstrap": [
      "https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.2/js/bootstrap.min",
      "lib/bootstrap/dist/js/bootstrap"
    ],
    "angular": [
      "https://cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.11/angular.min",
      "lib/angular/angular"
    ],
    "Blob": [
      "https://cdn.jsdelivr.net/blob.js/0.1/Blob",
      "lib/Blob/Blob"
    ],
    "saveAs": [
      "https://cdn.jsdelivr.net/filesaver.js/0.2/FileSaver.min",
      "lib/FileSaver/FileSaver"
    ]
  },
  "mainDeps": [
    "domReady",
    "angular",
    "app/app",
    "bootstrap"
  ],
  "apis": [
    "fileReader",
    "storage",
    "fileSaver",
    "server"
  ],
  "plugins": {
    "clipboard": {
      "id": "com.verso.cordova.clipboard",
      "url": "https://github.com/VersoSolutions/CordovaClipboard"
    },
    "flashlight": {
      "id": "nl.x-services.plugins.flashlight",
      "url": "https://github.com/EddyVerbruggen/Flashlight-PhoneGap-Plugin.git"
    },
    "batterystatus": {
      "id": "org.apache.cordova.battery-status"
    },
    "device": {
      "id": "org.apache.cordova.device"
    },
    "vibration": {
      "id": "org.apache.cordova.vibration"
    },
    "appversion": {
      "id": "uk.co.whiteoctober.cordova.appversion",
      "url": "https://github.com/whiteoctober/cordova-plugin-app-version.git"
    },
    "networkinfo": {
      "id": "org.apache.cordova.network-information"
    },
    "dialogs": {
      "id": "org.apache.cordova.dialogs"
    },
    "actionsheet": {
      "id": "nl.x-services.plugins.actionsheet",
      "url": "https://github.com/EddyVerbruggen/cordova-plugin-actionsheet.git"
    },
    "splashscreen": {
      "id": "org.apache.cordova.splashscreen"
    }
  }
}
